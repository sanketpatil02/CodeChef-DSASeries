#include<bits/stdc++.h>
#include<cstring>

using namespace std;

int main(){
    int t;
    cin>>t;

    while(t--){
        int flag=0;
        string str;
        cin>>str;
        reverse(str.begin(), str.end()); 
        for(int i=0;i<str.length();i++){
            if(str[i]=='0' && flag==0){

            }
            else{
                cout<<str[i];
                flag=1;
            }
        }
        cout<<endl;       
    }
}
